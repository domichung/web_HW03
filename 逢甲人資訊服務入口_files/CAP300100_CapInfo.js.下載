function iniCapData_Json300100(){if(typeof($("#capInfo300100").attr("id")) != "undefined"){var div = document.createElement('div');var ul = document.createElement('ul');ul.id='portfolio';var ClassName = '';
                                var ColorName = '';
                                var AlertImage = '';
                                $.each(capdata300100, function (value, item) {
                                    var template = $('#ScriptTemp_300100').html();
                                    ClassName = '';
                                    switch (item.EStatus) {
                                        case '0': // normal
                                            ClassName = 'bannerYellow';
                                            if (_300100__warningStyle != '')
                                                ColorName = 'background-color:' +_300100__warningStyle;
                                            break;
                                        case '1': // event
                                            ClassName = '';
                                            if (_300100__emergencyStyle != '')
                                                ColorName = 'background-color:' + _300100__emergencyStyle;
                                            break;
                                        case '2': // nothing
                                            ClassName = 'bannerGreen';
                                            if (_300100__peaceStyle != '')
                                                ColorName = 'background-color:' + _300100__peaceStyle;
                                            break;
                                        default:

                                    }
                                    AlertImage = GetBannerImg(item.Code);

                                    $(ul).append(template.replace(/{{@Code}}/g, item.Code)
                                        .replace(/{{@Event}}/g, item.Event)
                                        .replace(/{{@ClassName}}/g, ClassName)
                                        .replace(/{{@ColorName}}/g, ColorName)
                                        .replace(/{{@AlertImage}}/g, AlertImage)
                                        .replace(/{{@Time}}/g, item.Time)
                                        .replace(/{{@OrgName}}/g, item.OrgName)
                                        .replace(/{{@GetFormatSummaryBanner_300_100}}/g, item.GetFormatSummaryBanner_300_100)
                                        .replace(/{{@LinkMore}}/g, item.LinkMore)
                                    );
                                });$(div).append(ul);$("#capInfo300100").html("");$("#capInfo300100").append($(div).html());
								$('ul#portfolio').innerfade({speed: 2000,timeout: 5000,type: 'sequence',containerheight: '100px'});
								}
}
setInterval(AjaxCall_Json300100, 60000 );
						function AjaxCall_Json300100() {
							$.ajax({
								url: "https://alerts.ncdr.nat.gov/Service/Service.ashx?op=ListAlertAtomFeed&Fmat=Json300100",
								async: "true",
								dataType: "jsonp",
								method: "GET",
								jsonp: "callback",
								jsonpCallback:"doCallSuccess_Json300100"
							});
						}
						function doCallSuccess_Json300100(result){
							if (typeof (result) != "undefined" && result != null) {
								capdata300100 = result;
								iniCapData_Json300100();
							}
						}
						 function GetBannerImg(Code){
								try{
                                        if(Code == '' || Code == null)
                                            return 'https://alerts.ncdr.nat.gov.tw/Images/todayIsGoodDay.png';
                                        else
                                            return 'https://alerts.ncdr.nat.gov.tw/Images/calamityIcon150Black_'+Code+'.png?version=1'
													
								}
								catch(e){
									return '';
								}
							}